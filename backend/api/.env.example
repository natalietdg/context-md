# NestJS API environment variables
# Copy to .env and fill values

PORT=4000
SEALION_API_KEY=your-sealion-api-key-here
ENVIRONMENT=development
ALLOWED_API_KEYS=key1,key2,key3
API_KEY_REQUIRED=false

# Authentication & Security
JWT_SECRET=your-jwt-secret-key-here
DB_ENCRYPTION_KEY=your-32-character-encryption-key-here
API_KEY=contextmd-dev-api-key-2024

# AWS Configuration for Secrets Manager
AWS_REGION=ap-southeast-1
AWS_SECRET_NAME=your-rds-secret-name
S3_BUCKET_REGION=ap-northeast-2

# Legacy Database Configuration (fallback)
DATABASE_HOST=your-database-host.region.rds.amazonaws.com
DATABASE_PORT=5432
DATABASE_NAME=contextmd
DATABASE_USER=your-db-username
DATABASE_PASSWORD=your-db-password
DATABASE_SSL=true

REACT_APP_SUPABASE_ENDPOINT_URL=
REACT_APP_SUPABASE_ANON_KEY=
GENERATE_SOURCEMAP=false
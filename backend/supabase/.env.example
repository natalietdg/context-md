# Supabase Edge Functions environment variables
# Copy this file to .env and fill in the values

# Sea Lion API Key for https://api.sea-lion.ai
SEA_LION_API_KEY=

# Optional: include rawSeaLionData in responses when set to development
ENVIRONMENT=development

# Optional API key gating for your functions
# Comma-separated list of allowed x-api-key header values
ALLOWED_API_KEYS=
# Set to true to require x-api-key
API_KEY_REQUIRED=false

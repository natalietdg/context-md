container_commands:
  01_download_wheelhouse:
    command: "aws s3 sync s3://your-bucket/wheelhouse/ /tmp/wheelhouse/"
    leader_only: true
  02_install_from_wheelhouse:
    command: "source /var/app/venv/*/bin/activate && pip install --no-index --find-links /tmp/wheelhouse/ -r requirements.txt"
    leader_only: true
  03_install_whisperx:
    command: "source /var/app/venv/*/bin/activate && pip install --no-index --find-links /tmp/wheelhouse/ whisperx"
    leader_only: true
